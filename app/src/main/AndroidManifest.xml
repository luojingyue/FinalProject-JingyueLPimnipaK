<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.awos.finalproject_jingyuepim">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AuthenticationActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".MainActivity" />
        <activity
            android:name=".KoreaActivity"
            android:label="@string/title_activity_korea"
            android:theme="@style/AppTheme.NoActionBar"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".LebanonActivity"
            android:label="@string/title_activity_lebanon"
            android:theme="@style/AppTheme.NoActionBar"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".PRActivity"
            android:label="@string/title_activity_pr"
            android:theme="@style/AppTheme.NoActionBar"
            android:parentActivityName=".MainActivity"/>

        <activity
            android:name=".KoreanInfo"
            android:label="@string/title_activity_korean_info"
            android:theme="@style/AppTheme.NoActionBar"
            android:parentActivityName=".KoreaActivity"></activity>

    </application>

</manifest>